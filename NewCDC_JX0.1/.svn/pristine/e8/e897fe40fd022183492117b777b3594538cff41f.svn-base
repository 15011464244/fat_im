	/**
	 * 创建一个bean集合
	 * 
	 * @param list
	 * @param sign_sts_code
	 * @param undlv_cause_code
	 * @param undlv_next_actn_code
	 * @param signer_name
	 * @param pay_mode
	 * @param queueType
	 *            妥投传Constant.TUOTOU,未妥投传Constant.WEITUOTOU
	 * @param context
	 * @return
	 */
	public static ArrayList<DeliverQueueBean> createBeanList(
			ArrayList<MessageInfoBean> list, String sign_sts_code,
			String undlv_cause_code, String undlv_next_actn_code,
			String signer_name, String pay_mode, int queueType, Context context) {
		ArrayList<DeliverQueueBean> queueList = new ArrayList<DeliverQueueBean>();
		for (int i = 0; i < list.size(); i++) {
			MessageInfoBean bean = list.get(i);
			queueList.add(createBean(bean, sign_sts_code, bean.getMoney(),
					undlv_cause_code, undlv_next_actn_code, signer_name,
					pay_mode, queueType, context));
		}

		return queueList;
	}

	/**
	 * 创建一个queuebean
	 * 
	 * @param bean
	 * @param sign_sts_code
	 * @param actual_goods_fee
	 * @param undlv_cause_code
	 * @param undlv_next_actn_code
	 * @param signer_name
	 * @param pay_mode
	 * @param queueType
	 *            妥投传Constant.TUOTOU,未妥投传Constant.WEITUOTOU
	 * @param context
	 * @return
	 */
	public static DeliverQueueBean createBean(MessageInfoBean bean,
			String sign_sts_code, String actual_goods_fee,
			String undlv_cause_code, String undlv_next_actn_code,
			String signer_name, String pay_mode, int queueType, Context context) {
		UserInfoUtils user = new UserInfoUtils(context);
		String orgCode = user.getUserDelvorgCode();
		String userName = user.getUserName();
		String realName = user.getRealname();
		String deviceId = Utils.getDeviceId(context);
		DeliverQueueBean queueBean = null;
		switch (queueType) {
		case Constant.TUOTOU:
			queueBean = new DeliverQueueBean(bean.getMail_num(),
					bean.getDlv_date(), bean.getDlv_time(), orgCode,
					bean.getDlv_pseg_code(), "TT", sign_sts_code,
					actual_goods_fee, userName, realName, undlv_cause_code,
					undlv_next_actn_code, signer_name, deviceId, pay_mode,
					bean.getFlag(), Utils.getCurrTime(), bean.getFrequence(),
					Constant.UNCOMMIT);
			queueBean.setRcver_phone(bean.getRcver_contact_phone1());
			queueBean.setSender_phone(bean.getSender_contact_phone1());
			queueBean.setPicture(bean.getPicture());
			break;

		case Constant.WEITUOTOU:
			queueBean = new DeliverQueueBean(bean.getMail_num(),
					bean.getDlv_date(), bean.getDlv_time(), orgCode,
					bean.getDlv_pseg_code(), "CRZT", sign_sts_code,
					actual_goods_fee, userName, realName, undlv_cause_code,
					undlv_next_actn_code, signer_name, deviceId, pay_mode,
					bean.getFlag(), Utils.getCurrTime(), bean.getFrequence(),
					Constant.UNCOMMIT);
			queueBean.setRcver_phone(bean.getRcver_contact_phone1());
			queueBean.setSender_phone(bean.getSender_contact_phone1());
			break;
		}
		return queueBean;
	}

