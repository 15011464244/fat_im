<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true" android:color="@color/white"><shape>
            <gradient android:angle="270" android:endColor="#009140" android:startColor="#009140" />

            <corners android:radius="3dp" />

            <stroke android:width="1dp" android:color="#009140" />
        </shape></item>
    <item android:state_pressed="true" android:color="@color/white"><shape>
            <gradient android:angle="270" android:endColor="#009140" android:startColor="#009140" />

            <corners android:radius="3dp" />

            <stroke android:width="1dp" android:color="#009140" />
        </shape></item>
    <item android:color="@color/gree_my"><shape>
            <gradient android:angle="270" android:endColor="#ffffff" android:startColor="#ffffff" />

            <corners android:radius="3dp" />

            <stroke android:width="1dp" android:color="#009140" />
        </shape></item>

</selector>