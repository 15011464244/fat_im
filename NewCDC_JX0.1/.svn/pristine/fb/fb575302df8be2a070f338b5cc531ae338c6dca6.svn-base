                            android:textColor="@color/black"
                            android:textSize="@dimen/textSizeSamll" />

                        <EditText
                            android:id="@+id/edt_mainmail"
                            android:layout_width="fill_parent"
                            android:layout_height="@dimen/text_h"
                            android:layout_weight="1"
