                                <TextView
                                    android:layout_width="@dimen/jiushidp"
                                    android:layout_height="fill_parent"
                                    android:gravity="right|center_vertical"
                                    android:text="县 : "
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/textSizeSamll" />

                                <RelativeLayout
