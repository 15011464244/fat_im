<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/settings_back"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/left_margin"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/imageview_img_item_list_customdeal"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="@dimen/bashidp"
                android:scaleType="fitCenter"
                android:src="@drawable/head_img" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/paddingleft"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="单号："
                        android:textColor="@color/black"
                        android:textSize="@dimen/item_list_2_textsize" />

                    <TextView
                        android:id="@+id/mailnum_customdeal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="2"
                        android:background="@drawable/edit_bac"
                        android:padding="@dimen/item_list_2_margin"
                        android:textColor="@color/dialog_text"
                        android:textSize="@dimen/item_list_2_textsize" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/paddingleft"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="发件人："
                        android:textColor="@color/black"
                        android:textSize="@dimen/item_list_2_textsize" />

                    <TextView
                        android:id="@+id/sendname_customdeal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="2"
                        android:background="@drawable/edit_bac"
                        android:padding="@dimen/item_list_2_margin"
                        android:textColor="@color/dialog_text"
                        android:textSize="@dimen/item_list_2_textsize" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/paddingleft"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1.3"
                android:gravity="right"
                android:text="发件人电话："
                android:textColor="@color/black"
                android:textSize="@dimen/item_list_2_textsize" />

            <TextView
                android:id="@+id/sendphone_customdeal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="3"
                android:background="@drawable/edit_bac"
                android:padding="@dimen/item_list_2_margin"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/item_list_2_textsize" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/paddingleft"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1.3"
                android:gravity="right"
                android:text="发件人地址："
                android:textColor="@color/black"
                android:textSize="@dimen/item_list_2_textsize" />

            <TextView
                android:id="@+id/sendaddress_customdeal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="3"
                android:background="@drawable/edit_bac"
                android:padding="@dimen/item_list_2_margin"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/item_list_2_textsize" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/paddingleft"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1.3"
                android:gravity="right"
                android:text="收件人："
                android:textColor="@color/black"
                android:textSize="@dimen/item_list_2_textsize" />

            <TextView
                android:id="@+id/rcvername_customdeal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="3"
                android:background="@drawable/edit_bac"
                android:padding="@dimen/item_list_2_margin"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/item_list_2_textsize" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/paddingleft"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1.3"
                android:gravity="right"
                android:text="收件人电话："
                android:textColor="@color/black"
                android:textSize="@dimen/item_list_2_textsize" />

            <TextView
                android:id="@+id/rcverphone_customdeal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="3"
                android:background="@drawable/edit_bac"
                android:padding="@dimen/item_list_2_margin"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/item_list_2_textsize" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/paddingleft"
            android:layout_marginTop="@dimen/paddingleft"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1.3"
                android:gravity="right"
                android:text="收件人地址："
                android:textColor="@color/black"
                android:textSize="@dimen/item_list_2_textsize" />

            <TextView
                android:id="@+id/rcveraddress_customdeal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="3"
                android:background="@drawable/edit_bac"
                android:padding="@dimen/item_list_2_margin"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/item_list_2_textsize" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/sign_state_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/paddingleft"
            android:layout_marginTop="@dimen/paddingleft"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1.3"
                android:gravity="right"
                android:text="签收情况："
                android:textColor="@color/black"
                android:textSize="@dimen/item_list_2_textsize" />

            <TextView
                android:id="@+id/sign_state_customdeal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="3"
                android:background="@drawable/edit_bac"
                android:padding="@dimen/item_list_2_margin"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/item_list_2_textsize" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/error_layout_item_list_customdeal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/paddingleft"
                android:layout_marginTop="@dimen/paddingleft"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.3"
                    android:gravity="right"
                    android:text="未妥投原因："
                    android:textColor="@color/black"
                    android:textSize="@dimen/item_list_2_textsize" />

                <TextView
                    android:id="@+id/error_reason_item_list_customdeal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="3"
                    android:background="@drawable/edit_bac"
                    android:padding="@dimen/item_list_2_margin"
                    android:textColor="@color/dialog_text"
                    android:textSize="@dimen/item_list_2_textsize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/paddingleft"
                android:layout_marginTop="@dimen/paddingleft"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.3"
                    android:gravity="right"
                    android:text="下一步动作："
                    android:textColor="@color/black"
                    android:textSize="@dimen/item_list_2_textsize" />

                <TextView
                    android:id="@+id/error_nextstep_item_list_customdeal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="3"
                    android:background="@drawable/edit_bac"
                    android:padding="@dimen/item_list_2_margin"
                    android:textColor="@color/dialog_text"
                    android:textSize="@dimen/item_list_2_textsize" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/dlv_date_layout_item_list_customdeal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/paddingleft"
            android:layout_marginTop="@dimen/paddingleft"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1.3"
                android:gravity="right"
                android:text="投递日期："
                android:textColor="@color/black"
                android:textSize="@dimen/item_list_2_textsize" />

            <TextView
                android:id="@+id/dlv_date_item_list_customdeal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="3"
                android:background="@drawable/edit_bac"
                android:padding="@dimen/item_list_2_margin"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/item_list_2_textsize" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>