<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/selector_task_list"
    android:descendantFocusability="blocksDescendants" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/activity_horizontal_margin"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="姓名：\t\t"
                android:textColor="@color/black"
                android:textSize="@dimen/textSizeNormal" />

            <TextView
                android:id="@+id/tv_name_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/name"
                android:text="张三"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/textSizeNormal" />

            <TextView
                android:id="@+id/tv_remainTime_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="50分钟"
                android:textColor="@color/gree_my"
                android:textSize="@dimen/textSizeNormal" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/pading10"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/tel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="电话：\t\t"
                android:textColor="@color/black"
                android:textSize="@dimen/textSizeNormal" />

            <TextView
                android:id="@+id/tv_mobile_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/tel"
                android:text="12312312312"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/textSizeNormal" />

            <TextView
                android:id="@+id/dan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tel"
                android:layout_marginTop="@dimen/pading10"
                android:text="邮件号："
                android:textColor="@color/black"
                android:textSize="@dimen/textSizeNormal" />

            <FrameLayout
                android:id="@+id/frame_sms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/pading10" >

                <Button
                    android:id="@+id/btn_msg_item_list_delivertask"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/wudp"
                    android:layout_marginTop="@dimen/wudp"
                    android:background="@drawable/sms_black_big"
                    android:focusable="false" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|top" >

                    <Button
                        android:id="@+id/btn_msgcount_item_list_delivertask"
                        android:layout_width="@dimen/marginbottom_count"
                        android:layout_height="@dimen/marginbottom_count"
                        android:background="@drawable/red_circle_small"
                        android:clickable="false"
                        android:focusable="false"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/textSizemin"
                        android:visibility="gone" />
                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/frame_tel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/pading10"
                android:layout_toLeftOf="@+id/frame_sms" >

                <Button
                    android:id="@+id/btn_call_item_list_delivertask"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/wudp"
                    android:layout_marginTop="@dimen/wudp"
                    android:background="@drawable/call_black_big"
                    android:focusable="false" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="right|top" >

                    <Button
                        android:id="@+id/btn_callcount_item_list_delivertask"
                        android:layout_width="@dimen/marginbottom_count"
                        android:layout_height="@dimen/marginbottom_count"
                        android:background="@drawable/red_circle_small"
                        android:clickable="false"
                        android:focusable="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/textSizemin"
                        android:visibility="gone" />
                </LinearLayout>
            </FrameLayout>

            <TextView
                android:id="@+id/tv_mailNum_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/dan"
                android:layout_alignBottom="@+id/dan"
                android:layout_alignLeft="@+id/tv_mobile_item_list_delivertask"
                android:text="1231231231231"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/textSizeNormal" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/buttonHeight"
            android:layout_marginTop="@dimen/pading10"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/add"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="地址：\t\t"
                android:textColor="@color/black"
                android:textSize="@dimen/textSizeNormal" />

            <TextView
                android:id="@+id/tv_address_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_toRightOf="@+id/add"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/textSizeNormal" />

            <Button
                android:id="@+id/btn_state_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:background="@color/transparent"
                android:gravity="center"
                android:text="未上传"
                android:textColor="@color/red"
                android:textSize="@dimen/textSizeNormal" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="@dimen/buttonHeight"
            android:layout_marginTop="@dimen/pading10"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/huo"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="应收金额："
                android:gravity="center_vertical"
                android:textColor="@color/black"
                android:textSize="@dimen/textSizeNormal" />

            <TextView
                android:id="@+id/tv_money_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_toRightOf="@+id/huo"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/textSizeNormal" />

            <TextView
                android:id="@+id/tv_state_item_list_delivertask"
                android:layout_width="@dimen/baidp"
                android:layout_height="@dimen/buttonHeight"
                android:layout_alignParentRight="true"
                android:gravity="center"
                android:text="妥投"
                android:textColor="@color/gree_my"
                android:textSize="@dimen/textSizeNormal" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/tongxun"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/pading10"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/btn_selfgroup_item_list_delivertask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:background="@drawable/add_black_big"
                android:focusable="false" />

            <Button
                android:id="@+id/btn_tuotou_item_list_delivertask"
                android:layout_width="0dp"
                android:layout_height="@dimen/buttonHeight"
                android:layout_marginLeft="@dimen/pading10"
                android:layout_marginRight="@dimen/margin4"
                android:layout_weight="1"
                android:background="@drawable/selector_deliver_task"
                android:focusable="false"
                android:text="本人收"
                android:textColor="@drawable/selector_btn" />

            <Button
                android:id="@+id/btn_weituotou_item_list_delivertask"
                android:layout_width="0dp"
                android:layout_height="@dimen/buttonHeight"
                android:layout_marginLeft="@dimen/pading10"
                android:layout_marginRight="@dimen/margin4"
                android:layout_weight="1"
                android:background="@drawable/selector_deliver_task"
                android:focusable="false"
                android:text="未妥投"
                android:textColor="@drawable/selector_btn" />

            <Button
                android:id="@+id/btn_deal_item_list_delivertask"
                android:layout_width="0dp"
                android:layout_height="@dimen/buttonHeight"
                android:layout_marginLeft="@dimen/pading10"
                android:layout_marginRight="@dimen/margin4"
                android:layout_weight="1"
                android:background="@drawable/selector_deliver_task"
                android:focusable="false"
                android:text="其它"
                android:textColor="@drawable/selector_btn" />
        </LinearLayout>

        <ImageView
            android:id="@+id/chooseimge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone" />
    </LinearLayout>

</LinearLayout>