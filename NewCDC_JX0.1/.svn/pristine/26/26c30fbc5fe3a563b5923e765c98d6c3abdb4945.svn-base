<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/jiqian_money_remarks"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/title_dialog_userexit"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:layout_margin="@dimen/map_margin"
        android:gravity="center"
        android:text="注销"
        android:textColor="@color/black"
        android:textSize="@dimen/textSizeBig" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/map_divider"
        android:background="@color/glay" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/login_middle_margin"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/map_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="您有揽投数据未上传，确定退出"
                android:textColor="@color/dialog_text"
                android:textSize="@dimen/textSizeNormal" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/login_middle_margin"
            android:gravity="center"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/jude_cancel"
                style="@style/button_sel"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_margin="@dimen/dialog_marginBottom"
                android:layout_weight="1"
                android:background="@drawable/button_selecter"
                android:text="取消"
                android:textSize="@dimen/textSizeNormal" />

            <Button
                android:id="@+id/jude_sure"
                style="@style/button_sel"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_margin="@dimen/dialog_marginBottom"
                android:layout_weight="1"
                android:background="@drawable/button_selecter"
                android:text="确定"
                android:textSize="@dimen/textSizeNormal" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>