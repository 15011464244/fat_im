<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="#eee">

    <include
        android:id="@+id/title"
        layout="@layout/title_layout" />
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="9dp">
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="111dp"
            android:orientation="horizontal">
            
            <LinearLayout
	            android:id="@+id/ll_serverpoint"
	            android:layout_width="0dp"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:gravity="center"
	            android:orientation="vertical"
	            android:layout_margin="1dp"
	            android:background="#fff" 
	            android:visibility="gone">
	
	            <ImageView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="15dp"
	                android:layout_weight="20"
	                android:gravity="center_horizontal"
	                android:padding="10dp"
	                android:src="@drawable/img_serverpoint" />
	
	            <TextView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="8dp"
	                android:layout_weight="11"
	                android:gravity="center_horizontal"
	                android:text="服务网点"
	                android:textSize="16sp" />
	            
        	</LinearLayout>
        	
            <LinearLayout
	            android:id="@+id/ll_rangequery"
	            android:layout_width="0dp"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:gravity="center"
	            android:orientation="vertical"
	            android:layout_margin="1dp"
	            android:background="#fff" >
	
	            <ImageView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="15dp"
	                android:layout_weight="20"
	                android:padding="10dp"
	                android:gravity="center_horizontal"
	                android:src="@drawable/img_rangequery" />
	
	            <TextView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="8dp"
	                android:layout_weight="11"
	                android:gravity="center_horizontal"
	                android:text="收送范围"
	                android:textSize="16sp" />
	            
        	</LinearLayout>
        	<LinearLayout
	            android:id="@+id/ll_timequery"
	            android:layout_width="0dp"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:gravity="center"
	            android:orientation="vertical"
	            android:layout_margin="1dp"
	            android:background="#fff"
	            android:visibility="gone" >
	
	            <ImageView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="15dp"
	                android:layout_weight="20"
	                android:padding="10dp"
	                android:gravity="center_horizontal"
	                android:src="@drawable/img_timequery" />
	
	            <TextView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="8dp"
	                android:layout_weight="11"
	                android:gravity="center_horizontal"
	                android:text="时效查询"
	                android:textSize="16sp" />
        	</LinearLayout>
        	<LinearLayout
	            android:id="@+id/ll_pricequery"
	            android:layout_width="0dp"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:gravity="center"
	            android:orientation="vertical"
	            android:layout_margin="1dp"
	            android:background="#fff" >
	
	            <ImageView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="15dp"
	                android:layout_weight="20"
	                android:padding="10dp"
	                android:gravity="center_horizontal"
	                android:src="@drawable/img_pricequery" />
	
	            <TextView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="8dp"
	                android:layout_weight="11"
	                android:gravity="center_horizontal"
	                android:text="运费查询"
	                android:textSize="16sp" />
	            
        	</LinearLayout>
        </LinearLayout>
        
         <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="111dp"
            android:orientation="horizontal">
            
            <LinearLayout
	            android:id="@+id/ll_pricequery1"
	            android:layout_width="0dp"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:gravity="center"
	            android:orientation="vertical"
	            android:layout_margin="1dp"
	            android:background="#fff" 
	            android:visibility="gone">
	
	            <ImageView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="15dp"
	                android:layout_weight="20"
	                android:padding="10dp"
	                android:gravity="center_horizontal"
	                android:src="@drawable/img_pricequery" />
	
	            <TextView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="8dp"
	                android:layout_weight="11"
	                android:gravity="center_horizontal"
	                android:text="运费查询"
	                android:textSize="16sp" />
	            
        	</LinearLayout>
        	
            <LinearLayout
	            android:layout_width="0dp"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:gravity="center"
	            android:orientation="vertical"
	            android:layout_margin="1dp" >
	
	            <!-- <ImageView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="15dp"
	                android:layout_weight="20"
	                android:padding="10dp"
	                android:gravity="center_horizontal"
	                android:src="@drawable/img_rangequery" />
	
	            <TextView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="8dp"
	                android:layout_weight="11"
	                android:gravity="center_horizontal"
	                android:text="收送范围"
	                android:textSize="16sp" /> -->
	            
        	</LinearLayout>
        	<LinearLayout
	            android:layout_width="0dp"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:gravity="center"
	            android:orientation="vertical"
	            android:layout_margin="1dp" 
	            android:visibility="gone">
	
	            <!-- <ImageView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="15dp"
	                android:layout_weight="20"
	                android:padding="10dp"
	                android:gravity="center_horizontal"
	                android:src="@drawable/img_timequery" />
	
	            <TextView
	                android:layout_width="match_parent"
	                android:layout_height="0dp"
	                android:layout_marginTop="8dp"
	                android:layout_weight="11"
	                android:gravity="center_horizontal"
	                android:text="时效查询"
	                android:textSize="16sp" /> -->
	            
        	</LinearLayout>
        		
        </LinearLayout>
        
    </LinearLayout>
    
</LinearLayout>